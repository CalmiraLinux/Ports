{
    "name": "autoconf",
    "version": "2.71",
    "description": "Программы для воспроизведения сценариев командной оболочки, которые могут выполнять автоматическую настройку исходного кода из определенного пользовательского файла-шаблона. Они также необходимы для повторной компиляции пакета после обновления процедур сборки.",
    "site": "https://ftp.gnu.org/gnu/autoconf",
    "maintainer": "Michail Krasnov <linuxoid85@gmail.com>",
    "priority": "system",
    "deps": {
        "required": [
            "bash",
            "coreutils",
            "grep",
            "m4",
            "make",
            "perl",
            "sed",
            "texinfo"
        ],
        "runtime": [
            "bash",
            "coreutils",
            "grep",
            "m4",
            "make",
            "sed",
            "texinfo"
        ],
        "test": [
            "automake",
            "diffutils",
            "findutils",
            "gcc",
            "libtool"
        ],
        "before": [
            "automake"
        ],
        "optional": [
            "emacs"
        ]
    },
    "bins": [
        "autoconf",
        "autoheader",
        "autom4te",
        "autoreconf",
        "autoscan",
        "autoupdate",
        "ifnames"
    ],
    "dirs": [
        "/usr/share/autoconf"
    ]
}
