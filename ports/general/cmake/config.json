{
    "name": "cmake",
    "version": "1.15.9",
    "description": "The CMake package contains a modern toolset used for generating Makefiles. It is a successor of the auto-generated configure script and aims to be platform- and compiler-independent. A significant user of CMake is KDE since version 4.",
    "site": "https://cmake.org/",
    "priority": "user",
    "maintainer": "Michail Linuxoid85 Krasnov <linuxoid85@gmail.com>",
    "deps": {
        "required": [
            "general/libuv"
        ],
        "recommend": [
            "basicnet/curl",
            "general/libarchive",
            "basicnet/nghttp2-1.43.0"
        ],
        "optional": [
            "general/gcc",
            "general/git",
            "general/mercurial",
            "x/qt",
            "general/subversion"
        ]
    },
    "bins": [
        "ccmake",
        "cmake",
        "cmake-gui",
        "cpack",
        "ctest"
    ],
    "dirs": [
        "/usr/share/cmake-3.19",
        "/usr/share/doc/cmake-3.19.5"
    ]
}
